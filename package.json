{
  "name": "slate-react-dnd-plugin",
  "version": "1.0.12",
  "description": "A plugin to integrate react-dnd into the slate.js ecosphere.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": ["/node_modules/", "/example"]
  },
  "babel": {
    "presets": ["env", "react"]
  },
  "scripts": {
    "test": "jest",
    "build": "node_modules/.bin/tsc",
    "watch": "node_modules/.bin/tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jensneuse/slate-react-dnd-plugin.git"
  },
  "keywords": ["slate", "slate-react", "react-dnd", "react", "drag", "drop", "drag&drop", "dnd", "typescript", "ts"],
  "author": "Jens Neuse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jensneuse/slate-react-dnd-plugin/issues"
  },
  "homepage": "https://github.com/jensneuse/slate-react-dnd-plugin#readme",
  "dependencies": {
    "@types/react": "^16.0.34",
    "@types/react-dnd": "^2.0.34",
    "@types/react-dnd-html5-backend": "^2.1.8",
    "@types/react-dom": "^16.0.3",
    "@types/recompose": "^0.24.4",
    "immutable": "^3.8.2",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "recompose": "^0.26.0",
    "slate": "^0.32.1",
    "slate-react": "^0.11.1"
  },
  "devDependencies": {
    "@types/prop-types": "^15.5.2",
    "babel-jest": "^22.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "create-react-app": "^1.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.3.0",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.4",
    "typescript": "^2.6.2"
  }
}
